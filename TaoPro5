ListALL=[]
stt=0
while True:
	stt+=1
	cookiesFB=input(f"Điền Cookie FB Cần Reg {stt} :")
	if len(cookiesFB) > 10 :
		proxy=input(f"Proxy {stt} : ")
		print("ĐANG KIỂM TRA COOKIE ......",end='\r')
		headers = {
			'authority': 'www.facebook.com',
			'accept': 'text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9',
			'accept-language': 'vi',
			'cookie': cookiesFB,
			'sec-ch-prefers-color-scheme': 'light',
			'sec-ch-ua': '"Chromium";v="106", "Google Chrome";v="106", "Not;A=Brand";v="99"',
			'sec-ch-ua-mobile': '?0',
			'sec-ch-ua-platform': '"Windows"',
			'sec-fetch-dest': 'document',
			'sec-fetch-mode': 'navigate',
			'sec-fetch-site': 'none',
			'sec-fetch-user': '?1',
			'upgrade-insecure-requests': '1',
			'user-agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/106.0.0.0 Safari/537.36',
			'viewport-width': '1366',
		}
		url_profile = requests.get('https://www.facebook.com/me', headers=headers).url
		profile = requests.get(url_profile, headers=headers).text
		try:
			fb_dtsgFB = profile.split('{"name":"fb_dtsg","value":"')[1].split('"},')[0]
		except:
			fb_dtsgFB = profile.split(',"f":"')[1].split('","l":null}')[0]
		AE=f'{fb_dtsgFB}|{cookiesFB}|{proxy}'
		ListALL.append(AE)
		print("Thành Công ! ")
	else:
		break

dem = 0
print('                                               ',end='\r')

dl = int(input("Điền Delay :"))
def delay(dl):
   t=datetime.now().strftime("%H:%M")
   for ti in range(int(dl) , 0, -1):
   	print(f"</> Đang Delay Tạo Pro5 {ti}                    ",end='\r')

   	sleep(1)
